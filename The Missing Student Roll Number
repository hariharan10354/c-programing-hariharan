#include <stdio.h>

int main() {
    int n, i, sum = 0, arr_sum = 0;
    scanf("%d", &n);
    
    for(i = 1; i <= n; i++)
        sum += i;  // sum of all roll numbers from 1 to N
    
    int num;
    for(i = 0; i < n-1; i++) {
        scanf("%d", &num);
        arr_sum += num;  // sum of given roll numbers
    }
    
    printf("%d", sum - arr_sum);  // missing roll number
    
    return 0;
}
