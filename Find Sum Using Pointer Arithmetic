#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    long long arr[n];
    long long sum = 0;
    int i;

    for(i = 0; i < n; i++) {
        scanf("%lld", &arr[i]);
    }

    long long *ptr = arr;

    for(i = 0; i < n; i++) {
        sum += *(ptr + i);
    }

    printf("%lld", sum);

    return 0;
}
