#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    int arr[N];
    int i;
    int sum1 = 0, sum2 = 0;

    for(i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }

    for(i = 0; i < N/2; i++) {
        sum1 += arr[i];
    }

    for(i = N/2; i < N; i++) {
        sum2 += arr[i];
    }

    if(sum1 == sum2)
        printf("BALANCED");
    else
        printf("NOT BALANCED");

    return 0;
}
